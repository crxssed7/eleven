{
  "name": "eleven",
  "version": "1.0.0",
  "description": "11ty default template",
  "scripts": {
    "start:eleventy": "npx @11ty/eleventy --serve",
    "start:tailwind": "npx tailwindcss -i ./assets/css/styles.css -o ./_site/assets/css/styles.css --watch",
    "start": "concurrently \"npm run start:eleventy\" \"npm run start:tailwind\"",
    "build:css": "npx tailwindcss -i ./assets/css/styles.css -o ./_site/assets/css/styles.css",
    "build:html": "npx @11ty/eleventy",
    "build": "npm run build:html && npm run build:css"
  },
  "author": "crxssed",
  "dependencies": {
    "@11ty/eleventy": "^2.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "tailwindcss": "^3.4.3"
  }
}
